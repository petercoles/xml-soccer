{
    "name": "petercoles/xmlsoccer",
    "description": "An API client for the XML Soccer service",
    "keywords": [ "XML Soccer", "API", "Football Data" ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7",
        "guzzlehttp/guzzle": "~6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "PeterColes\\XmlSoccer\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
